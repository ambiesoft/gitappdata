    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Test Run",
            "program": "${workspaceFolder}\\getgitappdata.js",
            "args": [
                "${workspaceFolder}\\config/default.json",
                "-o",
                "defaultout.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "With -o",
            "program": "${workspaceFolder}\\getgitappdata.js",
            "args": [
                "${workspaceFolder}\\config/test.json",
                "-o",
                "testout.json",
                // "-t",
                // "retrycopy"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "With -o and -t",
            "program": "${workspaceFolder}\\getgitappdata.js",
            "args": [
                "${workspaceFolder}\\config/test.json",
                "-o",
                "testout.json",
                "-t",
                "smartmv"
            ]
        }
    ]
}